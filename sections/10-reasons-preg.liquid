    <script>
      document.addEventListener("DOMContentLoaded", function () {
        // Function to format MST date
        function formatDateInMST(date) {
          const options = { month: "long", day: "numeric", timeZone: "America/Denver" };
          return date.toLocaleDateString("en-US", options);
        }
    
        // Function to calculate the next 3 PM MST
        function getNext3PMInMST() {
          const now = new Date();
          const mstDate = new Date(new Date().toLocaleString("en-US", { timeZone: "America/Denver" })); // MST current date
          mstDate.setHours(15, 0, 0, 0); // Set to 3 PM MST
    
          if (now > mstDate) {
            mstDate.setDate(mstDate.getDate() + 1); // Move to next day if 3 PM is already passed
          }
    
          return mstDate;
        }
    
        // Initialize current MST date and target time
        const currentDateElement = document.querySelector("#shopify-section-template--15943232159803__pm_product_section_q4jQR9 .current-date");
        const timerElement = document.querySelector("#shopify-section-template--15943232159803__pm_product_section_q4jQR9 .timer");
    
        function updateDisplay() {
          const targetTime = getNext3PMInMST();
          const currentMSTDate = formatDateInMST(targetTime); // The date of the next 3 PM MST
    
          // Update the current MST date display
          currentDateElement.textContent = currentMSTDate;
    
          // Update the timer
          function updateTimer() {
            const currentTime = new Date();
            const timeDifference = targetTime - currentTime;
    
            if (timeDifference <= 0) {
              timerElement.textContent = "0hr 0mins";
              return;
            }
    
            const hours = Math.floor((timeDifference / (1000 * 60 * 60)) % 24);
            const minutes = Math.floor((timeDifference / (1000 * 60)) % 60);
    
            timerElement.textContent = `${hours}hr ${minutes}mins`;
          }
    
          updateTimer();
          setInterval(updateTimer, 1000); // Update every second
        }
      if(currentDateElement){
          updateDisplay();
      }
      
      });
    </script>
    
    
    
    
    
