    document.addEventListener("DOMContentLoaded", function() {
      function startCountdown(endTimeString) {
        const counterElement = document.querySelector("#shopify-section-template--15943232159803__pm_product_section_q4jQR9 #deal-counter");
        
        // Parse the date string to ensure consistency
        let endTime = new Date(endTimeString).getTime();
       
        if (isNaN(endTime)) {
          console.error("Invalid end time provided for the countdown.");
          counterElement.innerHTML = "Invalid Date";
          return;
        }
        function updateCountdown() {
          let now = new Date().getTime();
          let distance = endTime - now;
          const unendingTimer = false;
          if (distance < 0) {
            if(unendingTimer){
            // endTime = endTime + 10000;
              endTime = endTime + 86400000;
            distance = endTime - now;
            } else{
                counterElement.innerHTML = "00:00:00";
                clearInterval(interval); // Stop the countdown when it ends
            }
           
          
            
            return;
          }
    
    
          const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
          const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
          const seconds = Math.floor((distance % (1000 * 60)) / 1000);
    
          counterElement.innerHTML = `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
        }
    
        updateCountdown(); // Update immediately
        const interval = setInterval(updateCountdown, 1000); // Update every second
      }
    
      // Fetch the end time from the Liquid variable
      const countdownEndTime = "2025-05-14 07:00:00";
    
      // Start the countdown with the provided end time
      startCountdown(countdownEndTime);
    });
